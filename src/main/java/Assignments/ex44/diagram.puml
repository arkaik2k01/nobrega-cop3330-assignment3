@startuml
'https://plantuml.com/class-diagram

class App {
}

class products {
    -String name
    -double price
    -int quantity

    + Products(string, double, int)
    + gets()
    + sets()
}

class Shop {
    - ArrayList<Products>

    + jsonRead()
    + addToList(String, double, int)
    + gets()
    + sets()
}
@enduml